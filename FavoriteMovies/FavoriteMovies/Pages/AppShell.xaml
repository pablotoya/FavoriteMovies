<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FavoriteMovies.Pages.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pg="clr-namespace:FavoriteMovies.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="FavoriteMovies"
    Shell.NavBarHasShadow="False"
    Shell.BackgroundColor="#273746"
    Shell.TitleColor="White"
    Shell.ForegroundColor="White">

    

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate pg:FavoritePage}"
        Route="FavoritePage" />

        <ShellContent
        Title="Local"
        ContentTemplate="{DataTemplate pg:LocalFavoritePage}"
        Route="LocalFavoritePage" />

        
</Shell>
